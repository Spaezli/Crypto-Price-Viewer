<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CryptoPriceViewerView">
   <children>
      <VBox>
         <children>
            <HBox>
               <children>
                  <Button fx:id="btn_update" mnemonicParsing="false" text="Button" />
                  <Label fx:id="lbl_status" alignment="CENTER_RIGHT" text="Label" />
               </children>
            </HBox>
            <TableView fx:id="tbl_requests" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
